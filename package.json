{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "upgrade:scss": "yarn upgrade @indigoichigo/scss",
    "upgrade:network": "yarn upgrade @indigoichigo/network",
    "server": "cd ./nodeServer && yarn dev",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "plop:comp": "plop  --plopfile ./plop/executor/comp.js",
    "plop:view": "plop  --plopfile ./plop/executor/view.js",
    "genComp": "node generateComponent.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "commit": "node commit.js",
    "release": "standard-version && git push --follow-tags origin master && git add ."
  },
  "dependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@indigoichigo/network": "https://gitlab.com/p-libs/network.git",
    "@indigoichigo/scss": "https://gitlab.com/p-libs/scss.git",
    "async-validator": "^3.5.1",
    "chalk": "^4.1.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.4",
    "element-ui": "^2.15.1",
    "inquirer": "^8.1.1",
    "lodash": "^4.17.21",
    "quill": "^1.3.7",
    "quill-delta-to-html": "^0.12.0",
    "quill-delta-to-markdown": "^0.6.0",
    "register-service-worker": "^1.7.1",
    "shelljs": "^0.8.4",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-clickaway": "^2.2.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/vue": "^6.1.21",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "standard-version": "^9.3.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "vue-template-compiler": "^2.6.11"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{scss,css}": [
      "yarn lint:style"
    ],
    "*.{html,css,sccc,js,jsx,tsx,vue}": [
      "yarn lint:prettier"
    ]
  }
}
